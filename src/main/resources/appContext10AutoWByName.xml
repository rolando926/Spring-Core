<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="objClassA" class="SpringCore10_AutoWByName.ClassA">
        <property name="strMessage" value="This is ClassA with Setter."/>
    </bean>

    <bean id="objClassB" class="SpringCore10_AutoWByName.ClassB">
        <property name="strMessage" value="This is ClassB with Setter."/>
    </bean>


    <bean id="classCAuto" class="SpringCore10_AutoWByName.ClassC" autowire="byName"/>


</beans>